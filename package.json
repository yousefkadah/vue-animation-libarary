{
  "name": "@yousefkadah/vue-magic-ui",
  "version": "1.0.1",
  "description": "A Vue.js animated component library with beautiful, ready-to-use components",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "style": "dist/style.css",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yousefkadah/vue-animation-libarary.git"
  },
  "homepage": "https://yousefkadah.github.io/vue-animation-libarary/",
  "bugs": {
    "url": "https://github.com/yousefkadah/vue-animation-libarary/issues"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:lib": "vue-tsc && vite build --mode lib",
    "build:demo": "vite build",
    "preview": "vite preview",
    "deploy": "npm run build:demo && gh-pages -d dist",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "keywords": [
    "vue",
    "animation",
    "components",
    "ui",
    "library",
    "typescript",
    "vue3",
    "animated",
    "magic",
    "effects"
  ],
  "author": "Yousef Kadah",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "gh-pages": "^6.3.0",
    "typescript": "^5.2.0",
    "vite": "^5.0.0",
    "vitepress": "^1.0.0",
    "vue-tsc": "^1.8.0"
  },
  "dependencies": {
    "vue": "^3.0.0",
    "vue-router": "^4.5.1"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
